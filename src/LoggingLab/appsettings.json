{
  "Logging": {
    "LogLevel": { // For all providers,
      "Default": "Error", // Apply to all categories and set Minimum log level from Info to critial, not showing trace and debug
      "Microsoft": "Trace",
      "LoggingLab": "Information", // Should use namespace to specify the category
      "Microsoft.Hosting.Lifetime": "Trace"
    },

    "Console": { // Specifying provider
      "IncludeScopes": true,
      "Default": "Information", // For all providers, minimum log level from Info to critial, not showing trace and debug
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    },

    "Debug": { // Specifying provider
      "Default": "Error", // For all providers, minimum log level from Info to critial, not showing trace and debug
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    },

    "EventSource": { // Specifying provider
      "Default": "Error", // For all providers, minimum log level from Info to critial, not showing trace and debug
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    },

    "EventLog": { // Specifying provider
      "Default": "Error", // For all providers, minimum log level from Info to critial, not showing trace and debug
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    }
  },
  "AllowedHosts": "*"
}
